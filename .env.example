# Copy this to .env and fill in your values
APP_NAME=Mangable
APP_VERSION=1.0.0
DEBUG=false

# Database
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/mangable

# JWT - generate with: openssl rand -hex 32
SECRET_KEY=change-me-to-a-secure-random-string

# Cover images base URL (e.g. CDN or S3 bucket)
COVER_URL_BASE=https://covers.yourdomain.com
